{
	"name": "abjad-convert",
	"version": "0.2.1-disable.local",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"files": [
		"dist/src/**/*.d.ts",
		"dist/src/**/*.js",
		"LICENSE",
		"CHANGELOG.md",
		"README.md"
	],
	"author": "Amer Harb",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/amerharb/abjad.git"
	},
	"homepage": "https://github.com/amerharb/abjad#readme",
	"url": "https://github.com/amerharb/abjad/issues",
	"email": "abjad@amerharb.com",
	"keywords": [
		"abjad",
		"phonetic",
		"arabic",
		"imperial-aramaic",
		"phoenician",
		"syriac",
		"ugaritic"
	],
	"license": "ISC",
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "tsc",
		"start": "node dist/index.js",
		"test": "jest",
		"posttest": "npx make-coverage-badge --report-path coverage/coverage-summary.json --output-path badges/coverage.svg",
		"dev": "ts-node src/index.ts",
		"lint": "npx eslint . --max-warnings 0",
		"lint:fix": "npx eslint . --fix",
		"prepublishOnly": "yarn lint && yarn build && yarn test"
	}
}
